{
  "name": "parcel-test",
  "version": "0.1.2",
  "description": "testing out parcel with react front-end, node back-end",
  "main": "index.js",
  "scripts": {
    "setup": "yarn && cd server && yarn",
    "start": "cd server && cross-env NODE_ENV=development npx babel-watch server.js",
    "prod": "cd server && cross-env NODE_ENV=production  npx babel-node  server.js",
    "pack": "parcel build src/index.js --out-dir server/public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "react-router",
    "react-router-menu",
    "node",
    "express",
    "parcel"
  ],
  "author": "Martin A. Jackson <martin.a.jackson@gmail.com> (http://martinjackson.github.io)",
  "homepage": "https://github.com/martinjackson/parcel-test#readme",
  "bugs": {
    "url": "https://github.com/martinjackson/parcel-test/issues",
    "email": "martin.a.jackson@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@version/react-router-v3": "^1.0.0",
    "antd": "^3.8.1",
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "immutable": "^3.8.2",
    "madge": "^3.2.0",
    "parcel-bundler": "^1.9.7",
    "react": "^16.4.2",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.4.2",
    "react-flexbox-grid": "^2.1.2",
    "react-markdown": "^3.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.54",
    "@babel/node": "7.0.0-beta.54",
    "@babel/preset-env": "7.0.0-beta.54",
    "@babel/preset-react": "7.0.0-beta.54",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "opn-cli": "^3.1.0",
    "typescript": "^3.0.1",
    "wait-on": "^2.1.0"
  }
}
