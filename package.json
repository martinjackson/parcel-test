{
  "name": "parcel-react-node",
  "version": "0.1.2",
  "description": "testing out parcel with node on the back-end",
  "main": "index.js",
  "scripts": {
    "setup": "yarn && cd server && yarn",
    "start": "cd server && cross-env NODE_ENV=development npx babel-watch server.js",
    "prod":  "cd server && cross-env NODE_ENV=production  npx babel-node  server.js",
    "pack": "parcel build src/index.js --out-dir server/public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "react-router",
    "react-router-menu",
    "node",
    "express",
    "parcel"
  ],
  "author": "Martin A. Jackson <martin.a.jackson@gmail.com> (http://martinjackson.github.io)",
  "homepage": "https://github.com/martinjackson/react-node-example#readme",
  "bugs": {
    "url": "https://github.com/martinjackson/react-node-example/issues",
    "email": "martin.a.jackson@gmail.com"
  },
  "license": "ISC",
  "dependencies": {
    "@version/react-router-v3": "^1.0.0",
    "antd": "^3.7.0",
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "immutable": "^3.8.2",
    "madge": "^3.2.0",
    "parcel-bundler": "^1.9.7",
    "react": "^16.4.1",
    "react-autobind": "^1.0.6",
    "react-dom": "^16.4.1",
    "react-flexbox-grid": "^2.1.2",
    "react-markdown": "^3.3.4",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "opn-cli": "^3.1.0",
    "style-loader": "^0.21.0",
    "typescript": "^2.9.2",
    "wait-on": "^2.1.0"
  }
